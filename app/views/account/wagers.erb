<h3>Wagers I'm Following:</h3>
<% unless @user.my("Market").empty? %>

    <div>
    <% @user.my("Market").each do |e| %>
        <a href ="<%= remove_track_path(kind: 'Market', model_id: e.id) %>"><%= image_tag "delete-person.png", size: "20x20" %></a>
        <%= link_to e.description, e %><br />

    <% end %>

<% else %>
    <h6>Not Tracking Any Wagers</h6>
<% end %>
</div>